{
  "name": "@un/react",
  "version": "1.18.91",
  "description": "WFP UI Kit",
  "license": "Apache-2",
  "main": "lib/index.js",
  "module": "es/index.js",
  "main:scss": "source/globals/styles.scss",
  "sideEffects": false,
  "scripts": {
    "build:legacy": "npm run copy-vendor-styles && npm run build:rollup && npm run build:script && npm run build:css && npm run build:cssmin // node scripts/build.js",
    "build": "yarn rollup -c --bundleConfigAsCjs",
    "clean": "rimraf es lib umd storybook-static build react-docgen.json",
    "copy-vendor-styles": "sh ./tasks/copy-vendor-styles.sh",
    "build:rollup": "rollup -c",
    "build:script": "node scripts/build.js",
    "whichtest": "node scripts/whichTest.js",
    "build:storybook": "rm -r -f docs && storybook build -o docs",
    "ci-check": "npm run prettier:diff && npm run lint && npm run test --runInBand && npm run test-ssr",
    "build:cssmin": "sass --load-path=node_modules --load-path=../../node_modules ./src/globals/scss/styles.scss ./assets/css/styles.min.css && postcss ./assets/css/styles.min.css --no-map -u autoprefixer -rs",
    "build:css": "sass --load-path=node_modules --load-path=../../node_modules  ./src/globals/scss/styles.scss ./assets/css/styles.css && postcss ./assets/css/styles.css --output ./assets/css/styles.css --use autoprefixer",
    "build:favicon": "convert ../logos/src/favicons/favicon-32x32.png ../logos/src/favicons/favicon-260x260.png -colors 256 docs/favicon.ico",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "analyze": "source-map-explorer 'es/index.js'",
    "generate:types": "tsc generateTypeDefinitions.tsx --esModuleInterop true --outDir .temp && node .temp/generateTypeDefinitions.js && rm -rf .temp",
    "lint": "eslint src",
    "precommit": "lint-staged && npm run lint",
    "prettier": "prettier --write \"**/*.{scss,css,js}\"",
    "prettier:staged": "prettier",
    "prettier:diff": "prettier --list-different \"**/*.{scss,css,js}\"",
    "semantic-release": "semantic-release",
    "dev": "npm run storybook",
    "storybook": "STORYBOOK_ASSETS=https://cdn.wfp.org/guides/ui/assets/v0.0.1/ STORYBOOK_NPM_VERSION=10.10.3 storybook dev -p 9000",
    "test": "jest",
    "test-ssr": "npm run build && node ssr-tests/*.js",
    "jest-w": "jest --watch"
  },
  "sass": {
    "includePaths": [
      "./node_modules"
    ]
  },
  "keywords": [
    "WFP",
    "World Food Programme",
    "UN",
    "United Nations",
    "react",
    "ui-kit",
    "components"
  ],
  "babel": {
    "presets": [
      "./scripts/env",
      "@babel/preset-react"
    ],
    "plugins": [
      "dev-expression",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-export-default-from"
    ]
  },
  "files": [
    ".storybook",
    "build",
    "config",
    "es",
    "lib",
    "src",
    "umd",
    "rollup.config.ts",
    "react-docgen.json"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@carbon/feature-flags": "^0.6.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "classnames": "2.2.6",
    "invariant": "^2.2.4",
    "react-datepicker": "^4.6.0",
    "react-popper-tooltip": "^4.1.0",
    "react-scripts": "^5.0.1",
    "react-transition-state": "^2.1.0",
    "warning": "^4.0.3",
    "window-or-global": "^1.0.1",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-mdx-gfm": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/node-logger": "^7.0.6",
    "@storybook/preset-create-react-app": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-webpack5": "^7.0.6",
    "@storybook/testing-library": "^0.1.0",
    "@tippy.js/react": "^3.1.1",
    "@types/mdx": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@un/babel-preset-wfp": "^0.1.23",
    "@un/icons-react": "^10.34.48",
    "@un/pictograms-react": "^11.14.35",
    "@un/styles": "^10.26.68",
    "@un/themes-core": "^0.0.28",
    "add": "^2.0.6",
    "all-contributors-cli": "^6.17.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "browserslist-config-carbon": "^10.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "css-loader": "^5.2.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "generic-names": "^2.0.1",
    "gzip-size": "^5.1.1",
    "handlebars": "^4.7.7",
    "iframe-resizer": "^4.3.6",
    "jest": "^26.2.2",
    "lerna": "^4.0.0",
    "lint-staged": "^10.2.11",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.27.0",
    "npm-which": "^3.0.1",
    "object-assign": "^4.1.1",
    "postcss": "^8.2.14",
    "postcss-cli": "^8.3.1",
    "postcss-custom-properties": "^11.0.0",
    "postcss-loader": "4",
    "prettier": "^2.8.7",
    "pretty": "^2.0.0",
    "prismjs": "^1.20.0",
    "promise": "^8.1.0",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.1",
    "react": "18.2.0",
    "react-dates": "^21.8.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "18.2.0",
    "react-dropzone": "^11.0.2",
    "react-final-form": "^6.5.1",
    "react-hook-form": "^7.20.5",
    "react-is": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-popper": "^2.2.3",
    "react-prism": "^4.3.2",
    "react-redux": "^7.2.1",
    "react-select": "^3.1.0",
    "react-table": "7.5.0",
    "react-test-renderer": "^16.13.1",
    "react-toastify": "^6.0.8",
    "react-truncate": "^2.4.0",
    "react-truncate-html": "^0.1.7",
    "recharts": "^2.0.0-beta.6",
    "redux-form": "^8.3.6",
    "redux-form-website-template": "^1.0.13",
    "regenerator-runtime": "^0.13.9",
    "requestanimationframe": "^0.0.23",
    "rimraf": "^3.0.2",
    "rollup": "3.9.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-strip-banner": "^3.0.0",
    "rtlcss": "^3.3.0",
    "sass": "^1.33.0",
    "sass-loader": "10",
    "semantic-release": "^17.2.3",
    "semantic-release-ado": "^1.2.2",
    "source-map-explorer": "^2.5.2",
    "story-description-loader": "^1.0.0",
    "storybook": "^7.0.6",
    "storybook-addon-designs": "^7.0.0-beta.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.0.0",
    "twig": "^1.15.4",
    "typescript": "^4.8.3",
    "validate-commit-msg": "^2.14.0",
    "wcag-contrast": "^3.0.0",
    "webpack": "^5.75.0",
    "whatwg-fetch": "^3.3.1",
    "xml2js": "^0.4.23",
    "yaml-loader": "^0.6.0",
    "yarn": "^1.22.19"
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.js": [
      "npm prettier",
      "npm lint",
      "git add"
    ],
    "*.{css,scss}": [
      "npm prettier",
      "git add"
    ]
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "Subject does not match subject pattern!"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-components"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wfp/ui.git"
  },
  "types": "es/index.d.ts",
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.js",
      "!src/components/**/*-story.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "html"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.js?(x)",
      "<rootDir>/**/?(*-)(spec|test).js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/config/jest/jsTransform.js",
      "^.+\\.s?css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/",
      "/config/",
      "/lib/",
      "/es/",
      "/cjs/"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "bundleSizeThreshold": 80000,
  "gitHead": "fc5c56e65baccb03fbb9d719856258e16d2814bd"
}
